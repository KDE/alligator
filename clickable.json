       "build": "cd ${BUILD_DIR} && cmake -DCMAKE_CXX_COMPILER=g++ -DCMAKE_PREFIX_PATH='${ROOT}/qt-host;${ROOT}/ecmprefix' -DCMAKE_INSTALL_PREFIX='${ROOT}/kf5host' ${SRC_DIR} && make -j24 && make install",
